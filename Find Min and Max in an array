# Ques. Find Min and max in an array.

Sol.

def getMaxElement(a):
        maximum = a[0]
        for al in a:
            if(al > maximum):
                maximum = al
        return maximum

def getMinElement(a):
        minimum = a[0]
        for al in a:
            if(al < minimum):
                minimum = al
        return minimum

a = [5,2,4,7,1,-1,10]
max = getMaxElement(a)
min = getMinElement(a)
print(max)
print(min)
